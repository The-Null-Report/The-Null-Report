<div class="paper card">
	<div class="card-header">
		<h2>Submit a New Paper</h2>
	</div>
	<form id="new-paper-form" class="card-body">
		<label class="form-label" for="paper-title">Title</label>
		<input type="text" name="paper-title" class="form-input" />

		<label class="form-label" for="paper-author">Author</label>
		<input type="text" name="paper-author" class="form-input" />

		<label class="form-label" for="paper-subject">Subject</label>
		<input type="text" name="paper-subject" class="form-input" />

		<button type="submit" class="btn">Submit</button>
	</form>
</div>
<script src="/js/createNewPaper.js"></script>