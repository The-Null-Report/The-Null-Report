{{#if loggedIn}}
{{!-- <label for="upload">Upload your document</label> --}}



  <form class="watchlist" id="watch"> Watchlist </form>

  {{!-- {{#each papers}}
  <a class="list-item flex-row justify-space-between" href="/dashboard{{id}}"></a>
  <ul> {{title}} </ul>
  </a>

  {{/each}} --}}
 

<div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
<div class="modal-dialog">
	<div class="modal-content">
	<div class="modal-header">
		<h5 class="modal-title" id="exampleModalLabel">Upload your document</h5>
		<button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
	</div>
	<div class="modal-body">
	{{!-- FORM STARTS --}}
	<div class="mb-3 row">
		<label for="title" class="col-sm-2 col-form-label">Title</label>
		<div class="col-sm-10">
		<input type="text" class="form-control" id="title">
		</div>
	</div>
	<div class="mb-3 row">
		<label for="author" class="col-sm-2 col-form-label">Author</label>
		<div class="col-sm-10">
		<input type="text" class="form-control" id="author">
		</div>
	</div>
		<div class="mb-3 row">
		<label for="subject" class="col-sm-2 col-form-label">Subject</label>
		<div class="col-sm-10">
		<input type="text" class="form-control" id="subject">
		</div>
	</div>
	{{!-- FORM ENDS --}}
	</div>
	<div class="modal-footer">
		<button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
	</div>
	</div>
</div>
</div>
{{/if}}